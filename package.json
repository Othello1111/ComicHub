{
  "name": "ComicHub",
  "version": "1.0.0",
  "description": "ComicHub",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "start:server": "npm run --prefix ./server dev",
    "build": "npm run build:server && npm run build:react",
    "build:react": "umi build",
    "build:server": "npm run --prefix ./server build",
    "fix": "npm run style -- --fix && npm run lint -- --fix",
    "style": "stylelint 'src/**/*.less' --syntax less",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src server",
    "test": "umi test --coverage",
    "test:watch": "umi test --watchAll",
    "readme": "npm run --prefix ./server readme"
  },
  "dependencies": {
    "antd": "^3.20.5",
    "dva": "^2.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "umi": "^2.8.15"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/react-test-renderer": "^16.8.3",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "stylelint": "^10.1.0",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.0.1",
    "umi-plugin-react": "^1.9.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run style  && npm run lint",
      "pre-push": "npm run test"
    }
  },
  "repository": "https://github.com/nusr/ComicHub",
  "keywords": [
    "umi",
    "Comic",
    "Downloader",
    "typescript"
  ],
  "author": {
    "name": "Steve Xu",
    "email": "stevexugc@gmail.com"
  },
  "license": "MIT"
}
