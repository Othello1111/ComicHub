{
  "name": "ComicHub",
  "version": "1.0.0",
  "description": "ComicHub",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "start:server": "npm run --prefix ./server dev",
    "build": "npm run build:server && npm run build:react",
    "build:react": "umi build",
    "build:server": "npm run --prefix ./server build",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src server",
    "test": "umi test --coverage",
    "test:watch": "umi test --watchAll --coverage",
    "readme": "npm run --prefix ./server readme"
  },
  "dependencies": {
    "antd": "^3.20.7",
    "dva": "^2.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "umi": "^2.8.16"
  },
  "devDependencies": {
    "@types/jest": "^24.0.16",
    "@types/react-test-renderer": "^16.8.3",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "umi-plugin-react": "^1.9.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "repository": "https://github.com/nusr/ComicHub",
  "keywords": [
    "umi",
    "Comic",
    "Downloader",
    "typescript"
  ],
  "author": {
    "name": "Steve Xu",
    "email": "stevexugc@gmail.com"
  },
  "license": "MIT"
}
